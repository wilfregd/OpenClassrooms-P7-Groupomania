{"ast":null,"code":"import postBox from \"@/components/posts/PostBox.vue\";\nimport axios from \"axios\";\nimport config from \"../axios-config\";\nexport default {\n  name: \"UserView\",\n  props: [\"user\"],\n  components: {\n    postBox\n  },\n\n  data() {\n    return {\n      pageUser: null\n    };\n  },\n\n  beforeCreate() {\n    //Obtention de l'utilisateur demandé\n    //this.$route.params.id\n    axios.get(\"http://localhost:8000/api/user/\" + this.$route.params.id, config).then(response => {\n      this.pageUser = JSON.parse(response.data);\n    }).catch(err => console.log(err));\n  }\n\n};","map":{"version":3,"mappings":"AAaA,OAAOA,OAAP,MAAoB,gCAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,KAAK,EAAE,CAAC,MAAD,CAFM;EAGbC,UAAU,EAAE;IACVL;EADU,CAHC;;EAMbM,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IADL,CAAP;EAGD,CAVY;;EAWbC,YAAY,GAAG;IACb;IACA;IACAP,KAAI,CACDQ,GADH,CACO,oCAAoC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAD9D,EACkEV,MADlE,EAEGW,IAFH,CAESC,QAAD,IAAc;MAClB,KAAKP,QAAL,GAAgBQ,IAAI,CAACC,KAAL,CAAWF,QAAQ,CAACR,IAApB,CAAhB;IACD,CAJH,EAKGW,KALH,CAKUC,GAAD,IAASC,OAAO,CAACC,GAAR,CAAYF,GAAZ,CALlB;EAMD;;AApBY,CAAf","names":["postBox","axios","config","name","props","components","data","pageUser","beforeCreate","get","$route","params","id","then","response","JSON","parse","catch","err","console","log"],"sourceRoot":"","sources":["C:\\Users\\Wilfre\\Desktop\\OC P7\\front-end\\src\\views\\UserView.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"pageUser\" class=\"post_container\" v-cloak>\r\n    <h2>Publications de {{ pageUser.prenom }} {{ pageUser.nom }}</h2>\r\n    <postBox\r\n      v-for=\"(post, i) in posts\"\r\n      :key=\"i\"\r\n      v-bind:post=\"posts[i]\"\r\n      v-bind:user=\"user\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport postBox from \"@/components/posts/PostBox.vue\";\r\nimport axios from \"axios\";\r\nimport config from \"../axios-config\";\r\n\r\nexport default {\r\n  name: \"UserView\",\r\n  props: [\"user\"],\r\n  components: {\r\n    postBox,\r\n  },\r\n  data() {\r\n    return {\r\n      pageUser: null,\r\n    };\r\n  },\r\n  beforeCreate() {\r\n    //Obtention de l'utilisateur demandé\r\n    //this.$route.params.id\r\n    axios\r\n      .get(\"http://localhost:8000/api/user/\" + this.$route.params.id, config)\r\n      .then((response) => {\r\n        this.pageUser = JSON.parse(response.data);\r\n      })\r\n      .catch((err) => console.log(err));\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}