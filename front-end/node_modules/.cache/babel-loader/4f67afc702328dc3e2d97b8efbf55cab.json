{"ast":null,"code":"// @ is an alias to /src\n//import HelloWorld from '@/components/HelloWorld.vue'\nimport axios from \"axios\";\nimport config from \"../axios-config\";\nexport default {\n  name: \"AuthView\",\n  components: {},\n\n  data() {\n    return {\n      display: 0,\n      //Login form\n      lEmail: \"\",\n      lPassword: \"\",\n      //Signup form\n      sNom: \"\",\n      sPrenom: \"\",\n      sEmail: \"\",\n      sPassword: \"\",\n      sConfPassword: \"\" //TODO manage password confirm\n\n    };\n  },\n\n  onMounted() {//TODO redirect to \"User\" page if already connected\n  },\n\n  methods: {\n    //Tabs\n    onClickLoginTab() {\n      this.display = 0;\n    },\n\n    onClickSignupTab() {\n      this.display = 1;\n    },\n\n    //Actions\n    onClickLogin() {\n      //Logging In\n      axios.post(\"http://localhost:8000/api/auth/login\", {\n        email: this.lEmail,\n        password: this.lPassword\n      }, config).then(response => {\n        if (response.status == 200) {\n          //Connecté, redirection\n          this.$router.push(\"user\");\n        }\n      });\n    },\n\n    onClickSignup() {\n      //Signing Up\n      axios.post(\"http://localhost:8000/api/auth/signup\", {\n        nom: this.sNom,\n        prenom: this.sPrenom,\n        email: this.sEmail,\n        password: this.sPassword\n      }, config).then(response => {\n        if (response.status == 201) {\n          //Inscrit et connecté, redirection\n          this.$router.push(\"user\");\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0FA;AACA;AACA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,CADJ;MAGL;MACAC,MAAM,EAAE,EAJH;MAKLC,SAAS,EAAE,EALN;MAOL;MACAC,IAAI,EAAE,EARD;MASLC,OAAO,EAAE,EATJ;MAULC,MAAM,EAAE,EAVH;MAWLC,SAAS,EAAE,EAXN;MAYLC,aAAa,EAAE,EAZV,CAYc;;IAZd,CAAP;EAcD,CAlBY;;EAmBbC,SAAS,GAAG,CACV;EACD,CArBY;;EAsBbC,OAAO,EAAE;IACP;IACAC,eAAe,GAAG