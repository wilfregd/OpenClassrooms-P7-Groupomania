{"ast":null,"code":"import axios from \"axios\";\nimport config from \"./axios-config\";\nexport default {\n  data() {\n    return {\n      user: {}\n    };\n  },\n\n  created() {\n    axios.get(\"http://localhost:8000/api/auth/isloggedin\", config).then(response => {\n      console.log(response);\n\n      if (response.data.message == 0) {\n        this.$router.push(\"auth\");\n      } else {\n        //Connecté, on obtiens l'utilisateur actuel\n        user = response.data;\n      }\n    });\n  },\n\n  methods: {\n    handleUpdateNavbar(value) {\n      this.nav_name = value;\n    },\n\n    onLogout() {\n      axios.get(\"http://localhost:8000/api/auth/logout\", config).then(response => {\n        console.log(response);\n        console.log(response.status);\n\n        if (response) {\n          this.$router.push(\"auth\");\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,gBAAnB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;IADD,CAAP;EAGD,CALY;;EAMbC,OAAO,GAAG;IACRJ,KAAI,CACDK,GADH,CACO,2CADP,EACoDJ,MADpD,EAEGK,IAFH,CAESC,QAAD,IAAc;MAClBC,OAAO,CAACC,GAAR,CAAYF,QAAZ;;MACA,IAAIA,QAAQ,CAACL,IAAT,CAAcQ,OAAd,IAAyB,CAA7B,EAAgC;QAC9B,KAAKC,OAAL,CAAaC,IAAb,CAAkB,MAAlB;MACF,CAFA,MAEO;QACL;QACAT,IAAG,GAAII,QAAQ,CAACL,IAAhB;MACF;IACD,CAVH;EAWD,CAlBY;;EAmBbW,OAAO,EAAE;IACPC,kBAAkB,CAACC,KAAD,EAAQ;MACxB,KAAKC,QAAL,GAAgBD,KAAhB;IACD,CAHM;;IAIPE,QAAQ,GAAG;MACTjB,KAAI,CACDK,GADH,CACO,uCADP,EACgDJ,MADhD,EAEGK,IAFH,CAESC,QAAD,IAAc;QAClBC,OAAO,CAACC,GAAR,CAAYF,QAAZ;QACAC,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACW,MAArB;;QACA,IAAIX,QAAJ,EAAc;UACZ,KAAKI,OAAL,CAAaC,IAAb,CAAkB,MAAlB;QACF;MACD,CARH;IASD;;EAdM;AAnBI,CAAf","names":["axios","config","data","user","created","get","then","response","console","log","message","$router","push","methods","handleUpdateNavbar","value","nav_name","onLogout","status"],"sourceRoot":"","sources":["C:\\Users\\Wilfre\\Desktop\\OC P7\\front-end\\src\\App.vue"],"sourcesContent":["<template>\n  <nav v-if=\"!$route.meta.hideDefaultTemplate\" v-cloak>\n    <img alt=\"Logo Groupomania\" src=\"./assets/logo/icon.svg\" />\n    <div id=\"nav__links\">\n      <router-link to=\"/\">Home</router-link> |\n      <router-link to=\"/User\">User</router-link> |\n      <router-link to=\"/Settings\">Settings</router-link>\n    </div>\n    <div id=\"nav__userInfo\">\n      <img\n        src=\"http://localhost:8000/img/user/default.jpg\"\n        alt=\"Photo de profil utilisateur\"\n      />\n      <h3>{{ user.nom }}</h3>\n      <button @click.prevent=\"onLogout\">Se déconnecter</button>\n    </div>\n  </nav>\n  <router-view @update-navbar=\"handleUpdateNavbar\" v-bind:user=\"user\" />\n</template>\n\n<script>\nimport axios from \"axios\";\nimport config from \"./axios-config\";\n\nexport default {\n  data() {\n    return {\n      user: {},\n    };\n  },\n  created() {\n    axios\n      .get(\"http://localhost:8000/api/auth/isloggedin\", config)\n      .then((response) => {\n        console.log(response);\n        if (response.data.message == 0) {\n          this.$router.push(\"auth\");\n        } else {\n          //Connecté, on obtiens l'utilisateur actuel\n          user = response.data;\n        }\n      });\n  },\n  methods: {\n    handleUpdateNavbar(value) {\n      this.nav_name = value;\n    },\n    onLogout() {\n      axios\n        .get(\"http://localhost:8000/api/auth/logout\", config)\n        .then((response) => {\n          console.log(response);\n          console.log(response.status);\n          if (response) {\n            this.$router.push(\"auth\");\n          }\n        });\n    },\n  },\n};\n</script>\n\n<style>\n@import \"./assets/styles/style.css\";\n</style>"]},"metadata":{},"sourceType":"module"}