{"ast":null,"code":"import postBox from \"@/components/posts/PostBox.vue\";\nimport axios from \"axios\";\nimport config from \"../axios-config\";\nexport default {\n  name: \"UserView\",\n  props: [\"user\"],\n  components: {\n    postBox\n  },\n\n  data() {\n    return {\n      pageUser: null,\n      posts: [],\n      isNotFound: false\n    };\n  },\n\n  beforeCreate() {\n    //Obtention de l'utilisateur demandé\n    //this.$route.params.id\n    axios.get(\"http://localhost:8000/api/user/\" + this.$route.params.id, config).then(response => {\n      switch (response.status) {\n        case 200:\n          this.pageUser = JSON.parse(response.data);\n          break;\n\n        case 404:\n          this.isNotFound = true;\n          break;\n\n        case 401:\n          this.$route.push(\"/auth\");\n          break;\n      }\n    }).catch(err => console.log(err)); //Obtention des posts\n\n    axios.get(\"http://localhost:8000/api/post/getallposts/\" + this.$route.params.id, config).then(response => {\n      console.log(response);\n      const posts = response.data;\n\n      if (posts) {\n        this.posts = posts.reverse();\n      } else {\n        this.noPost = true;\n      }\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,OAAP,MAAoB,gCAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,KAAK,EAAE,CAAC,MAAD,CAFM;EAGbC,UAAU,EAAE;IACVL;EADU,CAHC;;EAMbM,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IADL;MAELC,KAAK,EAAE,EAFF;MAGLC,UAAU,EAAE;IAHP,CAAP;EAKD,CAZY;;EAabC,YAAY,GAAG;IACb;IACA;IACAT,KAAI,CACDU,GADH,CACO,oCAAoC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAD9D,EACkEZ,MADlE,EAEGa,IAFH,CAESC,QAAD,IAAc;MAClB,QAAQA,QAAQ,CAACC,MAAjB;QACE,KAAK,GAAL;UACE,KAAKV,QAAL,GAAgBW,IAAI,CAACC,KAAL,CAAWH,QAAQ,CAACV,IAApB,CAAhB;UACA;;QAEF,KAAK,GAAL;UACE,KAAKG,UAAL,GAAkB,IAAlB;UACA;;QAEF,KAAK,GAAL;UACE,KAAKG,MAAL,CAAYQ,IAAZ,CAAiB,OAAjB;UACA;MAXJ;IAaD,CAhBH,EAiBGC,KAjBH,CAiBUC,GAAD,IAASC,OAAO,CAACC,GAAR,CAAYF,GAAZ,CAjBlB,EAHa,CAsBb;;IACArB,KAAI,CACDU,GADH,CAEI,gDAAgD,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAFvE,EAGIZ,MAHJ,EAKGa,IALH,CAKSC,QAAD,IAAc;MAClBO,OAAO,CAACC,GAAR,CAAYR,QAAZ;MACA,MAAMR,KAAI,GAAIQ,QAAQ,CAACV,IAAvB;;MAEA,IAAIE,KAAJ,EAAW;QACT,KAAKA,KAAL,GAAaA,KAAK,CAACiB,OAAN,EAAb;MACF,CAFA,MAEO;QACL,KAAKC,MAAL,GAAc,IAAd;MACF;IACD,CAdH;EAeD;;AAnDY,CAAf","names":["postBox","axios","config","name","props","components","data","pageUser","posts","isNotFound","beforeCreate","get","$route","params","id","then","response","status","JSON","parse","push","catch","err","console","log","reverse","noPost"],"sourceRoot":"","sources":["C:\\Users\\Wilfre\\Desktop\\OC P7\\front-end\\src\\views\\UserView.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"pageUser && !isNotFound\" class=\"post_container\" v-cloak>\r\n    <h2>Publications de {{ pageUser.prenom }} {{ pageUser.nom }}</h2>\r\n    <postBox\r\n      v-for=\"(post, i) in posts\"\r\n      :key=\"i\"\r\n      v-bind:post=\"posts[i]\"\r\n      v-bind:user=\"user\"\r\n    />\r\n  </div>\r\n  <div v-if=\"isNotFound\">\r\n    <h1>This user does not exist</h1>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport postBox from \"@/components/posts/PostBox.vue\";\r\nimport axios from \"axios\";\r\nimport config from \"../axios-config\";\r\n\r\nexport default {\r\n  name: \"UserView\",\r\n  props: [\"user\"],\r\n  components: {\r\n    postBox,\r\n  },\r\n  data() {\r\n    return {\r\n      pageUser: null,\r\n      posts: [],\r\n      isNotFound: false,\r\n    };\r\n  },\r\n  beforeCreate() {\r\n    //Obtention de l'utilisateur demandé\r\n    //this.$route.params.id\r\n    axios\r\n      .get(\"http://localhost:8000/api/user/\" + this.$route.params.id, config)\r\n      .then((response) => {\r\n        switch (response.status) {\r\n          case 200:\r\n            this.pageUser = JSON.parse(response.data);\r\n            break;\r\n\r\n          case 404:\r\n            this.isNotFound = true;\r\n            break;\r\n\r\n          case 401:\r\n            this.$route.push(\"/auth\");\r\n            break;\r\n        }\r\n      })\r\n      .catch((err) => console.log(err));\r\n\r\n    //Obtention des posts\r\n    axios\r\n      .get(\r\n        \"http://localhost:8000/api/post/getallposts/\" + this.$route.params.id,\r\n        config\r\n      )\r\n      .then((response) => {\r\n        console.log(response);\r\n        const posts = response.data;\r\n\r\n        if (posts) {\r\n          this.posts = posts.reverse();\r\n        } else {\r\n          this.noPost = true;\r\n        }\r\n      });\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}