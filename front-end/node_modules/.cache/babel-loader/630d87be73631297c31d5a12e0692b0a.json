{"ast":null,"code":"import axios from \"axios\";\nimport config from \"../axios-config\";\nexport default {\n  name: \"UserView\",\n  props: [\"user\"],\n\n  data() {\n    return {\n      pageUser: null,\n      posts: [],\n      isNotFound: false\n    };\n  },\n\n  mounted() {\n    //Obtention de l'utilisateur demandé\n    //this.$route.params.id\n    axios.get(\"http://localhost:8000/api/user/\" + this.$route.params.id, config).then(response => {\n      console.log(response);\n\n      if (response.status == 200) {\n        this.pageUser = JSON.parse(response.data); //Obtention des posts\n\n        axios.get(\"http://localhost:8000/api/post/getallposts/\" + this.$route.params.id, config).then(response => {\n          this.isNotFound = false;\n          const posts = response.data;\n\n          if (posts) {\n            this.posts = posts.reverse();\n          } else {\n            this.noPost = true;\n          }\n        });\n      } else {\n        this.isNotFound = true;\n      }\n    }).catch(err => {\n      this.isNotFound = true;\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAUA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,KAAK,EAAE,CAAC,MAAD,CAFM;;EAIbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IADL;MAELC,KAAK,EAAE,EAFF;MAGLC,UAAU,EAAE;IAHP,CAAP;EAKD,CAVY;;EAWbC,OAAO,GAAG;IACR;IACA;IACAR,KAAI,CACDS,GADH,CACO,oCAAoC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAD9D,EACkEX,MADlE,EAEGY,IAFH,CAESC,QAAD,IAAc;MAClBC,OAAO,CAACC,GAAR,CAAYF,QAAZ;;MACA,IAAIA,QAAQ,CAACG,MAAT,IAAmB,GAAvB,EAA4B;QAC1B,KAAKZ,QAAL,GAAgBa,IAAI,CAACC,KAAL,CAAWL,QAAQ,CAACV,IAApB,CAAhB,CAD0B,CAG1B;;QACAJ,KAAI,CACDS,GADH,CAEI,gDACE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAHzB,EAIIX,MAJJ,EAMGY,IANH,CAMSC,QAAD,IAAc;UAClB,KAAKP,UAAL,GAAkB,KAAlB;UACA,MAAMD,KAAI,GAAIQ,QAAQ,CAACV,IAAvB;;UAEA,IAAIE,KAAJ,EAAW;YACT,KAAKA,KAAL,GAAaA,KAAK,CAACc,OAAN,EAAb;UACF,CAFA,MAEO;YACL,KAAKC,MAAL,GAAc,IAAd;UACF;QACD,CAfH;MAgBF,CApBA,MAoBO;QACL,KAAKd,UAAL,GAAkB,IAAlB;MACF;IAC