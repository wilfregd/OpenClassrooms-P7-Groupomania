{"ast":null,"code":"import axios from \"axios\";\nimport config from \"./axios-config\";\nexport default {\n  data() {\n    return {\n      nav_name: \"\",\n      user: {}\n    };\n  },\n\n  created() {\n    axios.get(\"http://localhost:8000/api/auth/isloggedin\", config).then(response => {\n      if (response.data.message == 0) {\n        const jsonRes = JSON.parse(response.data);\n        this.$emit(\"update-navbar\", \"test\");\n      }\n    });\n  },\n\n  methods: {\n    handleUpdateNavbar(value) {\n      this.nav_name = value;\n    },\n\n    onLogout() {\n      axios.get(\"http://localhost:8000/api/auth/logout\", config).then(response => {\n        console.log(response);\n        console.log(response.status);\n\n        if (response) {\n          this.$router.push(\"auth\");\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,gBAAnB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,IAAI,EAAE;IAFD,CAAP;EAID,CANY;;EAObC,OAAO,GAAG;IACRL,KAAI,CACDM,GADH,CACO,2CADP,EACoDL,MADpD,EAEGM,IAFH,CAESC,QAAD,IAAc;MAClB,IAAIA,QAAQ,CAACN,IAAT,CAAcO,OAAd,IAAyB,CAA7B,EAAgC;QAC9B,MAAMC,OAAM,GAAIC,IAAI,CAACC,KAAL,CAAWJ,QAAQ,CAACN,IAApB,CAAhB;QACA,KAAKW,KAAL,CAAW,eAAX,EAA4B,MAA5B;MACF;IACD,CAPH;EAQD,CAhBY;;EAiBbC,OAAO,EAAE;IACPC,kBAAkB,CAACC,KAAD,EAAQ;MACxB,KAAKb,QAAL,GAAgBa,KAAhB;IACD,CAHM;;IAIPC,QAAQ,GAAG;MACTjB,KAAI,CACDM,GADH,CACO,uCADP,EACgDL,MADhD,EAEGM,IAFH,CAESC,QAAD,IAAc;QAClBU,OAAO,CAACC,GAAR,CAAYX,QAAZ;QACAU,OAAO,CAACC,GAAR,CAAYX,QAAQ,CAACY,MAArB;;QACA,IAAIZ,QAAJ,EAAc;UACZ,KAAKa,OAAL,CAAaC,IAAb,CAAkB,MAAlB;QACF;MACD,CARH;IASD;;EAdM;AAjBI,CAAf","names":["axios","config","data","nav_name","user","created","get","then","response","message","jsonRes","JSON","parse","$emit","methods","handleUpdateNavbar","value","onLogout","console","log","status","$router","push"],"sourceRoot":"","sources":["C:\\Users\\Wilfre\\Desktop\\OC P7\\front-end\\src\\App.vue"],"sourcesContent":["<template>\n  <nav v-i