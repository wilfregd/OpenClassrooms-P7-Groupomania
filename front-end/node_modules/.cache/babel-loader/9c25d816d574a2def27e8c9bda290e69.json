{"ast":null,"code":"import postBox from \"@/components/posts/PostBox.vue\";\nimport axios from \"axios\";\nimport config from \"../axios-config\";\nexport default {\n  name: \"HomeView\",\n  components: {\n    postBox\n  },\n\n  data() {\n    return {\n      posts: []\n    };\n  },\n\n  created() {\n    //Vérification d'une connection existante avec le serveur\n    axios.get(\"http://localhost:8000/api/auth/isloggedin\", config).then(response => {\n      if (response.data.message == 0) {\n        //Déconnecté, redirection\n        this.$router.push(\"auth\");\n      }\n    }); //Obtention des posts\n\n    axios.get(\"http://localhost:8000/api/post/getallposts\", config).then(response => {\n      this.posts = response.data;\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,OAAP,MAAoB,gCAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE;IACVJ;EADU,CAFC;;EAKbK,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IADF,CAAP;EAGD,CATY;;EAUbC,OAAO,GAAG;IACR;IACAN,KAAI,CACDO,GADH,CACO,2CADP,EACoDN,MADpD,EAEGO,IAFH,CAESC,QAAD,IAAc;MAClB,IAAIA,QAAQ,CAACL,IAAT,CAAcM,OAAd,IAAyB,CAA7B,EAAgC;QAC9B;QACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,MAAlB;MACF;IACD,CAPH,EAFQ,CAWR;;IACAZ,KAAI,CACDO,GADH,CACO,4CADP,EACqDN,MADrD,EAEGO,IAFH,CAESC,QAAD,IAAc;MAClB,KAAKJ,KAAL,GAAaI,QAAQ,CAACL,IAAtB;IACD,CAJH;EAKD;;AA3BY,CAAf","names":["postBox","axios","config","name","components","data","posts","created","get","then","response","message","$router","push"],"sourceRoot":"","sources":["C:\\Users\\Wilfre\\Desktop\\OC P7\\front-end\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div id=\"home\">\n    <h1>Posts Groupomania</h1>\n    <div class=\"newpost\">\n      <img\n        class=\"post__userPic\"\n        v-bind:src=\"`${user.profilePic}`\"\n        alt=\"Utilisateur {{user.prenom}} {{user.nom}}\"\n      />\n    </div>\n    <div v-if=\"posts.length > 0\" class=\"post_container\">\n      <postBox v-for=\"(post, i) in posts\" :key=\"i\" v-bind:post=\"posts[i]\" />\n    </div>\n    <div v-else>\n      <h1>No post</h1>\n    </div>\n  </div>\n</template>\n\n<script>\nimport postBox from \"@/components/posts/PostBox.vue\";\nimport axios from \"axios\";\nimport config from \"../axios-config\";\n\nexport default {\n  name: \"HomeView\",\n  components: {\n    postBox,\n  },\n  data() {\n    return {\n      posts: [],\n    };\n  },\n  created() {\n    //Vérification d'une connection existante avec le serveur\n    axios\n      .get(\"http://localhost:8000/api/auth/isloggedin\", config)\n      .then((response) => {\n        if (response.data.message == 0) {\n          //Déconnecté, redirection\n          this.$router.push(\"auth\");\n        }\n      });\n\n    //Obtention des posts\n    axios\n      .get(\"http://localhost:8000/api/post/getallposts\", config)\n      .then((response) => {\n        this.posts = response.data;\n      });\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}