{"ast":null,"code":"import postBox from \"@/components/posts/PostBox.vue\";\nimport axios from \"axios\";\nimport config from \"../axios-config\";\nexport default {\n  name: \"UserView\",\n  props: [\"user\"],\n  components: {\n    postBox\n  },\n\n  data() {\n    return {\n      pageUser: null,\n      posts: []\n    };\n  },\n\n  beforeCreate() {\n    //Obtention de l'utilisateur demandé\n    //this.$route.params.id\n    axios.get(\"http://localhost:8000/api/user/\" + this.$route.params.id, config).then(response => {\n      this.pageUser = JSON.parse(response.data);\n    }).catch(err => console.log(err)); //Obtention des posts\n\n    axios.get(\"http://localhost:8000/api/post/getallposts/\" + this.$route.params.id, config).then(response => {\n      console.log(response);\n      const posts = response.data;\n\n      if (posts) {\n        this.posts = posts.reverse();\n      } else {\n        this.noPost = true;\n      }\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAaA,OAAOA,OAAP,MAAoB,gCAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,KAAK,EAAE,CAAC,MAAD,CAFM;EAGbC,UAAU,EAAE;IACVL;EADU,CAHC;;EAMbM,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IADL;MAELC,KAAK,EAAE;IAFF,CAAP;EAID,CAXY;;EAYbC,YAAY,GAAG;IACb;IACA;IACAR,KAAI,CACDS,GADH,CACO,oCAAoC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAD9D,EACkEX,MADlE,EAEGY,IAFH,CAESC,QAAD,IAAc;MAClB,KAAKR,QAAL,GAAgBS,IAAI,CAACC,KAAL,CAAWF,QAAQ,CAACT,IAApB,CAAhB;IACD,CAJH,EAKGY,KALH,CAKUC,GAAD,IAASC,OAAO,CAACC,GAAR,CAAYF,GAAZ,CALlB,EAHa,CAUb;;IACAlB,KAAI,CACDS,GADH,CAEI,gDAAgD,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAFvE,EAGIX,MAHJ,EAKGY,IALH,CAKSC,QAAD,IAAc;MAClBK,OAAO,CAACC,GAAR,CAAYN,QAAZ;MACA,MAAMP,KAAI,GAAIO,QAAQ,CAACT,IAAvB;;MAEA,IAAIE,KAAJ,EAAW;QACT,KAAKA,KAAL,GAAaA,KAAK,CAACc,OAAN,EAAb;MACF,CAFA,MAEO;QACL,KAAKC,MAAL,GAAc,IAAd;MACF;IACD,CAdH;EAeD;;AAtCY,CAAf","names":["postBox","axios","config","name","props","components","data","pageUser","posts","beforeCreate","get","$route","params","id","then","response","JSON","parse","catch","err","console","log","reverse","noPost"],"sourceRoot":"","sources":["C:\\Users\\Wilfre\\Desktop\\OC P7\\front-end\\src\\views\\UserView.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"pageUser\" class=\"post_container\" v-cloak>\r\n    <h2>Publications de {{ pageUser.prenom }} {{ pageUser.nom }}</h2>\r\n    <postBox\r\n      v-for=\"(post, i) in posts\"\r\n      :key=\"i\"\r\n      v-bind:post=\"posts[i]\"\r\n      v-bind:user=\"user\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport postBox from \"@/components/posts/PostBox.vue\";\r\nimport axios from \"axios\";\r\nimport config from \"../axios-config\";\r\n\r\nexport default {\r\n  name: \"UserView\",\r\n  props: [\"user\"],\r\n  components: {\r\n    postBox,\r\n  },\r\n  data() {\r\n    return {\r\n      pageUser: null,\r\n      posts: [],\r\n    };\r\n  },\r\n  beforeCreate() {\r\n    //Obtention de l'utilisateur demandé\r\n    //this.$route.params.id\r\n    axios\r\n      .get(\"http://localhost:8000/api/user/\" + this.$route.params.id, config)\r\n      .then((response) => {\r\n        this.pageUser = JSON.parse(response.data);\r\n      })\r\n      .catch((err) => console.log(err));\r\n\r\n    //Obtention des posts\r\n    axios\r\n      .get(\r\n        \"http://localhost:8000/api/post/getallposts/\" + this.$route.params.id,\r\n        config\r\n      )\r\n      .then((response) => {\r\n        console.log(response);\r\n        const posts = response.data;\r\n\r\n        if (posts) {\r\n          this.posts = posts.reverse();\r\n        } else {\r\n          this.noPost = true;\r\n        }\r\n      });\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}