{"ast":null,"code":"import postBox from \"@/components/posts/PostBox.vue\";\nimport axios from \"axios\";\nimport config from \"../axios-config\";\nexport default {\n  name: \"UserView\",\n  props: [\"user\"],\n  components: {\n    postBox\n  },\n\n  data() {\n    return {\n      pageUser: null,\n      posts: [],\n      isNotFound: false\n    };\n  },\n\n  beforeMount() {\n    //Obtention de l'utilisateur demandé\n    //this.$route.params.id\n    axios.get(\"http://localhost:8000/api/user/\" + this.$route.params.id, config).then(response => {\n      console.log(response);\n\n      if (response.status == 200) {\n        this.pageUser = JSON.parse(response.data); //Obtention des posts\n\n        axios.get(\"http://localhost:8000/api/post/getallposts/\" + this.$route.params.id, config).then(response => {\n          console.log(response);\n          const posts = response.data;\n\n          if (posts) {\n            this.posts = posts.reverse();\n          } else {\n            this.noPost = true;\n          }\n        });\n      } else {\n        console.log(\"UNFOUND\");\n        this.isNotFound = true;\n      }\n    }).catch(err => {\n      this.isNotFound = true;\n      console.log(err);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,OAAP,MAAoB,gCAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,KAAK,EAAE,CAAC,MAAD,CAFM;EAGbC,UAAU,EAAE;IACVL;EADU,CAHC;;EAMbM,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IADL;MAELC,KAAK,EAAE,EAFF;MAGLC,UAAU,EAAE;IAHP,CAAP;EAKD,CAZY;;EAabC,WAAW,GAAG;IACZ;IACA;IACAT,KAAI,CACDU,GADH,CACO,oCAAoC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAD9D,EACkEZ,MADlE,EAEGa,IAFH,CAESC,QAAD,IAAc;MAClBC,OAAO,CAACC,GAAR,CAAYF,QAAZ;;MACA,IAAIA,QAAQ,CAACG,MAAT,IAAmB,GAAvB,EAA4B;QAC1B,KAAKZ,QAAL,GAAgBa,IAAI,CAACC,KAAL,CAAWL,QAAQ,CAACV,IAApB,CAAhB,CAD0B,CAG1B;;QACAL,KAAI,CACDU,GADH,CAEI,gDACE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAHzB,EAIIZ,MAJJ,EAMGa,IANH,CAMSC,QAAD,IAAc;UAClBC,OAAO,CAACC,GAAR,CAAYF,QAAZ;UACA,MAAMR,KAAI,G