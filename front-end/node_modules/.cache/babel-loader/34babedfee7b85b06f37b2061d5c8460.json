"http://localhost:8000/api/post/getallposts/\" +\r\n                this.$route.params.id,\r\n              config\r\n            )\r\n            .then((response) => {\r\n              console.log(response);\r\n              const posts = response.data;\r\n\r\n              if (posts) {\r\n                this.posts = posts.reverse();\r\n              } else {\r\n                this.noPost = true;\r\n              }\r\n            });\r\n        } else {\r\n          console.log(\"UNFOUND\");\r\n          this.isNotFound = true;\r\n        }\r\n      })\r\n      .catch((err) => {\r\n        this.isNotFound = true;\r\n        console.log(err);\r\n      });\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}